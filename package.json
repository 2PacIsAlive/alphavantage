    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "alpha vantage",
    "vantage",
    "alpha",
    "alphavantage",
    "stocks",
    "stock market",
    "api"
  ],
  "author": "Zack Urben <zackurben@gmail.com> (http://zackurben.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackurben/alphavantage"
  },
  "homepage": "https://github.com/zackurben/alphavantage",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^21.1.0",
    "prettier": "^1.7.3"
  },
  "jest": {
    "bail": false,
    "verbose": true,
    "testMatch": [
      "**/test/**.js"
    ]
